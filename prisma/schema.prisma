generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model Password {
  id         String   @id @default(uuid())
  content    String
  length     Int
  hasSymbols Boolean
  noDuplicates Boolean @default(false)
  createdAt  DateTime @default(now())
}

model User {
  id       String @id @default(uuid())
  username String @unique
  password String // Aqui ficar√° o HASH da senha mestre
}